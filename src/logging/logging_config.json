{
    "logger": {
        "path": "./data/logs",
        "filename": "access.log",
        "level": "info",
        "rotation": "1 day",
        "retention": "1 month",
        "format": "<green>{time:YYYY-MM-DD HH:mm:ss.SSS}</green> <level>{level: <8}</level> <cyan>{name}</cyan>:<cyan>{function}</cyan> <level>{message}</level>"
    }
}